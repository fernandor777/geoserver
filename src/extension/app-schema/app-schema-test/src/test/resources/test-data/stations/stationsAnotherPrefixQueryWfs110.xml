<?xml version="1.0" encoding="UTF-8"?>
<wfs:GetFeature service="WFS" version="1.1.0"
                xmlns:wfs="http://www.opengis.net/wfs"
                xmlns:ogc="http://www.opengis.net/ogc"
                xmlns:ms_gml31="http://www.measurements_gml31.org/1.0"
    			xmlns:stOther31="http://www.stations_gml31.org/1.0"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://www.opengis.net/wfs http://schemas.opengis.net/wfs/1.1.0/wfs.xsd">
      <wfs:Query typeName="stOther31:Station_gml31">
        <ogc:Filter>
              <ogc:PropertyIsEqualTo>
                <ogc:PropertyName>stOther31:Station_gml31/stOther31:name</ogc:PropertyName>
                <ogc:Literal>station1</ogc:Literal>
              </ogc:PropertyIsEqualTo>
          </ogc:Filter>  
      </wfs:Query>
    </wfs:GetFeature>